{
    "name": "prepack",
    "description": "Comando para previo al pack",
    "steps": [
      {
        "name": "Borra el paquete",
        "command": "rm -f autoforce*.tgz",
        "onError": "skip"
      },
      {
        "name": "Borra los archivos en Lib",
        "command": "rm -rf ./lib/",
        "onError": "skip"
      },
      {
        "name": "Compila el typescript",
        "command": "tsc --project tsconfig.build.json"
      }
    ]
  }
